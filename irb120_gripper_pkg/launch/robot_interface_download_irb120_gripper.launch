<!--
  Manipulator specific version of abb_driver's 'robot_interface.launch'.

  Defaults provided for IRB 120:
   - J23_coupled = false

  Usage:
    robot_interface_download_irb120.launch robot_ip:=<value>
-->
<launch>
  <arg name="robot_ip" doc="IP of the controller" />
  <arg name="J23_coupled" default="false" doc="If true, compensate for J2-J3 parallel linkage" />

  <rosparam command="load" file="$(find irb120_gripper_pkg)/config/joint_names.yaml" />

  <include file="$(find abb_driver)/launch/robot_interface.launch">
    <arg name="robot_ip"    value="$(arg robot_ip)" />
    <arg name="J23_coupled" value="$(arg J23_coupled)" />
  </include>

  <!-- QUANDO SARA' PRONTO IL DRIVER, PUBBLICIZZARE l'ACTION SERVER
  <include file="$(find robotiq_2f_85_driver)/launch/robotiq_2f_85_driver.launch"></include> 
  -->
</launch>
